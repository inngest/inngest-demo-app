{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES6", "DOM"],
    "module": "commonjs",
    "moduleResolution": "node",
    "types": ["node", "express"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "outDir": "./build"
  },
  "include": ["index.ts", "worker.ts", "./inngest/**/*", "./lib/**/*"]
}
