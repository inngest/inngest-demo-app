{
  "name": "inngest-test-app",
  "version": "2.0.0",
  "description": "An app for our Inngest demo account",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec 'ts-node -r dotenv/config index.ts'",
    "send": "ts-node -r dotenv/config ./scripts/sendEvents.ts"
  },
  "nodemonConfig": {
    "watch": [
      "index.ts",
      "**/*.ts"
    ],
    "ext": "ts"
  },
  "license": "MIT",
  "dependencies": {
    "casual": "^1.6.2",
    "dotenv": "^16.0.3",
    "express": "^4.19.2",
    "inngest": "^3.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.2",
    "nodemon": "^2.0.21",
    "ts-node": "^10.9.1",
    "typescript": "4.8.2"
  },
  "engine": {
    "node": ">=20",
    "pnpm": "^8.6.2"
  }
}
