{
  "name": "inngest-test-app",
  "version": "2.0.0",
  "description": "An app for our Inngest demo account",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch --env-file=.env index.ts",
    "send": "ts-node -r dotenv/config ./scripts/sendEvents.ts"
  },
  "nodemonConfig": {
    "watch": [
      "index.ts",
      "**/*.ts"
    ],
    "ext": "ts"
  },
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "casual": "^1.6.2",
    "dotenv": "^16.0.3",
    "express": "^4.19.2",
    "inngest": "^3.46.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.2",
    "tsx": "^4.20.6",
    "typescript": "5.9.3"
  },
  "engine": {
    "node": ">=20",
    "pnpm": "10.x"
  }
}
